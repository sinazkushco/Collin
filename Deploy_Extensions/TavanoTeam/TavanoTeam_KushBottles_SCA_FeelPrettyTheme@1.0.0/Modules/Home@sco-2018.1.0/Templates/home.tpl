<div class="home-top">
<!-- Main pods Section -->
<section class="top-section">
	<div class="top-section-container">
			<div class="main-pods" data-cms-area="main-pods" data-cms-area-filters="path">

      </div>
	</div>
</section>
</div>

<div class="home">
		<!-- Latest Categories -->
		<section class="latest-categories">
			<div class="row" data-cms-area="home_categories-content" data-cms-area-filters="path">

			</div>
		</section>

</div>







<!-- SECTION 3 SUB PRODUCTS -->
<section class="sub-products">
	<div class="sub-pods row" data-cms-area="sub-pods" data-cms-area-filters="path">

	</div>
</section>
<!--SECTION 3 SUB PRODUCTS -->




<!-- SECTION 4 BADGES WRAPPER -->
<section class="trust-badges-wrapper">
  <div class="container">
      <div class="row trust-badges" data-cms-area="trust-badges" data-cms-area-filters="path">

      </div>
  </div>
</section>





<!-- SECTION 5 PARALAX BANNER -->
<section class="parallax" style="background: url(&quot;/Images/Home/parallax/background-custom-section.gif&quot;)">
    <div class="container">
        <div class="row sec-custom-pkg" data-cms-area="sec-custom-pkg" data-cms-area-filters="path">

        </div>
    </div>
</section>




<!-- Start First Merchandising Zone -->
<section class="home-full-width homeMerchandising">
	<div class="home-wrappper">
		<div class="home-merchandizing-zone">
			<h3 class="home-title">Hot Selling Items</h3>
			<div class="section-title-merchandising" data-cms-area="home_merchandising-top-title" data-cms-area-filters="path">
			</div>
			<!--Merchandising zone-->
			<div class="home-cms-page-merchandising" data-cms-area="home_merchandising-top-list" data-cms-area-filters="path">
			</div>
		</div>
	</div>
</section>
<!-- End First Merchandising Zone -->








